#!/usr/bin/env node
/*
BEISPIEL
*/

let x;  //Eine Variable namens x deklarieren

/*
AUFGABEN BEREICH
###############################################

Beschreibe die Art des Datentyps

###############################################
*/


x = 1;              // Die Variable "x" wird mit dem numerischen Wert 1 intialisiert. (Number)
x = 0.01;           // Die Variable "x" wird mit dem numerischen Wert 0.01 intialisiert. (Number)
x = "hello world";  // Die Variable "x" wird mit einem Zeichenkettenwert (String) "hello world" intialisiert. (String)
x = 'JavaScript';   // Die Variable "x" wird erneut mit einem Zeichenkettenwert "JavaScript" intialisiert. (String)
x = true;           // Die Variable "x" wird auf den booleschen Wert "true" gesetzt. (Boolean)
x = false;          // Die Variable "x" wird auf den booleschen Wert "false" gesetzt. (Boolean)
x = null;           // Die Variable "x" wird auf den speziellen Wert "null" gesetzt. (Null)
x = undefined;      // Die Variable "x" wird auf den speziellen Wert "undefined" gesetzt. (Undefined)
