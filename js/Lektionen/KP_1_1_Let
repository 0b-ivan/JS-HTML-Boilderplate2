/*
BEISPIEL
*/

let x;  //Eine Variable namens x deklarieren

/*
AUFGABEN BEREICH
###############################################

Beschreibe die Art des Datentyps

###############################################
*/


x = 1;              // int
x = 0.01;           // int
x = "hello world";  // string
x = 'JavaScript';   // string
x = true;           // boolean
x = false;          // boolean
x = null;           // boolean
x = undefined;      // undefined

